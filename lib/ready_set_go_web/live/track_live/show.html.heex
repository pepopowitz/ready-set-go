<.header>
  Track <%= @event.name %>
</.header>

<ul id="msg-list" phx-update="stream">
  <%= for wave <- @event.waves do %>
    <li id={"wave-#{wave.id}"}>
      <b><%= wave.name %>:</b>
      <%= wave.start_time %>
      <button phx-click="update_start_time" phx-value-id={wave.id}>Update</button>
    </li>
  <% end %>
</ul>
